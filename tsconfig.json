{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "declaration": true
    },
    "include": ["src"],
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended",
    "exclude": [
        "node_modules"
    ]
  }